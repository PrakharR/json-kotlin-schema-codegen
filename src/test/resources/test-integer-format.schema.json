{
  "$schema": "http://json-schema.org/draft/2019-09/schema",
  "$id": "http://pwall.net/test-integer-format",
  "description": "Test integer formats.",
  "type": "object",
  "properties": {
    "little": {
      "type": "integer",
      "format": "int32"
    },
    "big": {
      "type": "integer",
      "format": "int64"
    }
